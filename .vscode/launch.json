{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/dist/app.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy",
                "--expose-gc"
            ],
            "env": {
                "NODE_ENV": "development",
                "DRONE": "dummy"
            },
            "console": "internalConsole",
            "sourceMaps": true
        }
    ]
}